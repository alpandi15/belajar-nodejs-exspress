{
  "name": "belajar-nodejs-express",
  "version": "1.0.0",
  "description": "Belajar node js dengan ExpressJS",
  "main": "index.js",
  "author": "Muhammad Al-Pandi",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.12",
    "sequelize": "^6.6.5"
  },
  "scripts": {
    "start": "nodemon --exec babel-node ./src/app.js",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "clean": "rm -rf dist",
    "create:model": "func() { npx sequelize-cli model:generate --name \"$1\" --attributes name:string; }; func",
    "migrate": "npx sequelize-cli db:migrate"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "sequelize-cli": "^6.2.0"
  }
}
