{
  "name": "belajar-nodejs-express",
  "version": "1.0.0",
  "description": "Belajar node js dengan ExpressJS",
  "main": "index.js",
  "author": "Muhammad Al-Pandi",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "mariadb": "^2.5.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.0",
    "nodemon": "^2.0.12",
    "pug": "^3.0.2",
    "sequelize": "^6.6.5"
  },
  "scripts": {
    "start": "nodemon --exec babel-node ./src/bin/server.js",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "clean": "rm -rf dist",
    "create:model": "func() { npx sequelize-cli model:generate --name \"$1\" --attributes name:string; }; func",
    "migrate": "npx sequelize-cli db:migrate",
    "migration:rollback": "npx sequelize-cli db:migrate:undo",
    "migration:rollback:all": "npx sequelize-cli db:migrate:undo:all"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "sequelize-cli": "^6.2.0"
  }
}
